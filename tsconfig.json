    "types": [
      "@cloudflare/workers-types"
    ],
    "lib": [
      "ES2022"
    ],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler"
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ]
}